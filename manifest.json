{
  "name": "Arbiter CalendarSync",
  "key": "",
  "version": "1.0",
  "description": "Add games from Arbiter to your Google calendar",
  "permissions": [
    "storage",
    "declarativeContent",
    "tabs",
    "https://accounts.google.com/o/oauth2/token",
    "identity"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "BackgroundScript.js",
      "services/CalendarService.js",
      "auth.js",
      "types/ScheduledGame.js",
      "HWRGame.js",
      "ARBGame.js",
      "lib/moment.js",
      "lib/jquery.min.js"
    ],
    "persistent": false
  },
  "oauth2": {
    "client_id": "439216554338-hs93a8g50qp7l349dfufua7spniknf6n.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar"
    ]
  },
  "content_security_policy": "script-src 'self' https://calendar.google.com https://apis.google.com; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "*://*.arbitersports.com/Official/GameScheduleEdit.aspx",
        "*://www.horizonwebref.com/?*pageID=1102*"
      ],
      "js": [
        "types/ScheduledGame.js",
        "HWRGame.js",
        "ARBGame.js",
        "types/CalEvent.js",
        "services/BackgroundListenerService.js",
        "main.js",
        "horizonContent.js",
        "arbiterContent.js",
        "lib/moment.js",
        "lib/jquery.min.js"
      ]
    }
  ],
  
  "manifest_version": 2
}